create table t (
  a int,
  b int without system versioning
) with system versioning;

insert into t values(1, 2);
set @p0= now(6);
update t set b= 4;

select * from t;
select * from t for system_time as of @p0;
select * from t for system_time as of @p0 order by b asc;

select count(*) from t for system_time as of @p0 group by b;
select * from t for system_time as of @p0 group by b having b = 4;
select * from t for system_time as of @p0 where b = 4;

drop table t;
